# include <stdio.h>
int main(){
    int marks ;
    scanf("%d",&marks);
    if (marks<30)
    {
        printf("grade is C \n");
    }
    else if(marks>=30 && marks<70)
    {
        printf("Grade is B \n");
    }
    else if (marks<=70 && marks<90)
    {
        printf("Grade is A \n") ; 
    }
    else if (marks<=90 && marks<100)
    {
        printf("grade is A++");
    }
    else{
        printf("invalid");
    }
    return 0;
}
