# include <stdio.h>
int main(){
    int n;
    do{
        printf("Enter the value :");
        scanf("%d",&n);
        if(n%7==0)
        {
            printf("%d is divisible by seven \n",n);
        }
        else
        {
          printf("thans for visit");
          break;
        }
        
    }while(1);
    
    
}
