//write a program to print smallest number of variable three
# include <stdio.h>
int main(){
    int num1,num2,num3;
    printf("Enter a three number : \n");
    scanf("%d %d %d",&num1,&num2,&num3);
    if (num1<num2 && num1<num3)
    {
        printf("number %d is small number ",num1);
    }
    else if (num2<num3)
    {
        printf("number %d is small number ",num2);
    }
    else
    {
        printf(" small number is %d",num3);
    }
    return 0;
}
